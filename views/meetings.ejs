    <!-- Content Wrapper. Contains page content -->
    <div class="content-wrapper">
      <!-- Content Header (Page header) -->
      <div class="content-header">
        <div class="container-fluid">
          <div class="row mb-2">
            <div class="col-sm-6">
              <h1 class="m-0 text-dark"><a href="index/#<%= room_n %>"class="back"><i class="fas fa-arrow-left"></i>&nbsp;Back to Dashboard</a></h1>
            </div><!-- /.col -->
            <div class="col-sm-6">
              <!-- <ol class="breadcrumb float-sm-right">
                <li class="breadcrumb-item"><a href="#">Home</a></li>
                <li class="breadcrumb-item active">Dashboard v1</li>
              </ol> -->
            </div><!-- /.col -->
          </div><!-- /.row -->
        </div><!-- /.container-fluid -->
      </div>
      <!-- /.content-header -->

      <!-- Main content -->
      <section class="content">
        <div class="row">
         
          <div class="col-md-6">
            <div class="card ml-3 ">
              <h2 class="main_title ml-2 mt-3">Meeting History</h2>
              <table class="table table-striped ">
                <thead>
                  <tr>
                    <th>Date</th>
                    <th>Start Time</th>
                    <th>End Time  </th>
                    <th>Duration</th>
                  </tr>
                </thead>
                <tbody>
                  <% for( var display of result) { %>
                    <% if(display.d_id == doc_id) { %>
                      <tr >
                        <td><%= moment(display.call_start).format( 'MMM-DD-YYYY') %></td>
                        <td><%= moment(display.call_start).format( 'HH:mm a') %></td>
                        <td><%= moment(display.call_end).format( 'HH:mm a') %></td>
                        <td>
                          <%= (moment.utc(moment(display.call_end,"HH:mm:ss").diff(moment(display.call_start,"HH:mm:ss"))).format("HH:mm:ss"))%>
                        </td>
                      </tr>
                    <% }  %>
                  <% }  %>
                </tbody>
              </table>
            </div>
          </div>
          <div class="col-md-3"></div>
          <div class="col-md-3"></div>
        </div>
   
      </section>
      <!-- /.content -->
