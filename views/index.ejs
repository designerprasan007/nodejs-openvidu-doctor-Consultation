         <div class="container myindexcontainer">
           <div class="card">
             <div class="card-body">
               <h1>Welcome, <%= initial %>.&nbsp;<%= name %></h1>
               <small class="small_text ">To invite someone to your meeting room, share this link</small>

               <!-- <form class="mt-2"> -->
               <div class="row text-center">
                 <!-- <div class="row"> -->
                 <div class="col-md-8">
                   <div class="input-group">
                     <input type="text" aria-label="First name" class="form-control inputbox myclass" id="copylink"
                       value="https://pontis.me:5060/index/#<%= room_name %>">
                     <div class="input-group-prepend ">
                       <span><button type="button" class="input-group-text copytext " id="share_link">Copy</button>
                       </span>

                     </div>
                   </div>
                 </div>
                 <div class="col-md-4  ">
                   <button type="button" class="btn btn-outline-info button_class" id="hideshow2">Invite via &nbsp;<i
                       class="fas fa-angle-down angle_icon"></i></button>
                 </div>
               </div>
               <div id="content2" class="shadow">

                 <ul class="list-group list-group-flush">
                   <li class="list-group-item liststyle"><button type="button" class="btn social_text"
                       data-toggle="modal" data-target="#exampleModalCenter"><i
                         class="far fa-envelope mr-3 social_icon"></i>Email</button></li>
                   <li class="list-group-item liststyle"><button type="button" class="btn social_text"
                       data-toggle="modal" data-target="#exampleModalCenter1"><i
                         class="fas fa-calendar-alt mr-3 social_icon"></i>Calendar</button> </li>
                   <li class="list-group-item liststyle"><button type="button" class="btn social_text"
                       data-toggle="modal" data-target="#exampleModalCenter2"><i
                         class="far fa-comments mr-3 social_icon"></i>Text Message</button></li>
                   <li class="list-group-item liststyle"><button type="button" class="btn social_text"
                       data-toggle="modal" data-target="#exampleModalCenter3"><i
                         class="fas fa-plus-square mr-3 social_icon"></i>Website badge</button></li>
                   <li class="list-group-item liststyle"><button type="button" class="btn social_text"
                       data-toggle="modal" data-target="#exampleModalCenter4"><i
                         class="fas fa-print mr-3 social_icon"></i>Prints</button></li>
                 </ul>
               </div>
               <!-- </div> -->
               <!-- </form> -->

             </div>
           </div>

           <!-- modal for scoial icons -->
           <div class="modal fade" id="exampleModalCenter" tabindex="-1" role="dialog"
             aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
             <div class="modal-dialog modal-dialog-centered" role="document">
