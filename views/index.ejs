           <div id="ellipsecontent">
             <div class="d-flex flex-column">
                <div class="settings_ellipse p-2" data-toggle="modal" data-target="#exampleModal">
                  <i class="fas fa-cog  p-2 mr-2"></i>Device settings
                </div>
                <div class="settings_ellipse p-2" onclick="muteAudio()">
                  <i class="fas fa-microphone  p-2 mr-2"></i>Mute Myself
                </div>
                <div class="settings_ellipse p-2" onclick="muteVideo()"> 
                  <i class="fas fa-video  p-2 mr-2"></i> Turn off camera
                </div>
               <div class="settings_ellipse p-2" id="hide_pr"> <i class="fas fa-expand-alt  p-2 mr-2"></i>Hide my preview</div>
             </div>
           </div>
           <!-- <div id="videos" class="row no-margin">
          </div> -->
