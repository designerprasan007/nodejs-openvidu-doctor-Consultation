                 <div class="modal-header">
                   <h5 class="modal-title" id="exampleModalLongTitle"><i class="far fa-envelope mr-3 "></i><span
                       class="inviteheading">Invite via Email</span> </h5>
                   <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                     <span aria-hidden="true">&times;</span>
                   </button>
                 </div>
                 <div class="modal-body mycontext">
                   <form>
                     <div class="form-check">
                       <input class="form-check-input checkinput" type="radio" name="exampleRadios" id="exampleRadios1 "
                         onclick="default_email()" value="option1">
                       <label class="form-check-label" for="exampleRadios1">
                         <span class="emailselection"> Use my default email client</span>
                       </label>
                     </div>

                     <div class="form-check mt-2">
                       <input class="form-check-input checkinput" type="radio" name="exampleRadios" id="exampleRadios2 "
                         onclick="webservice()" value="option2">
                       <label class="form-check-label" for="exampleRadios2">
                         <span class="emailselection"> Use web service (e.g. Gmail, Hotmail, etc.)</span>
                       </label>
                     </div>

                     <div class="form-check mt-2">
                       <input class="form-check-input checkinput" type="radio" name="exampleRadios" id="exampleRadios3 "
                         onclick="doxy_me()" value="option3 ">
                       <label class="form-check-label" for="exampleRadios3">
                         <span class="emailselection">Send from BellaPontiS</span>
                       </label>
                     </div>
                   </form>
                 </div>
                 <div class="modal-footer ">
                   <div id="default_email" class="none">
                     <button type="button" class="btn btnstyle " data-dismiss="modal">Cancel</button>
                     <a type="button" class="btn  btnstyle1" href="mailto:<%= email %>">start composing the email</a>
                   </div>
                   <div id="webservice" class="none">
                     <div class="d-flex">
                       <div class="link_item">
                         <a href="https://mail.google.com/mail/u/0/?view=cm&fs=1&tf=1&su=invitation for pontis&body=Hello,+this+is+<%=initial%>.+<%=name%>+-+please+join+me+for+a+secure+video+call:+%0D%0Dhttps://pontis.me:5060/index/<%= room_name %>+%0D%0DUse+a+computer+or+device+with+a+good+internet+connection+and+webcam.+If+you+run+into+issues+connecting,+restart+your+computer+or+check+out+the+pontis.me+http://help.pontis.me+%0D%0DSimple,+free,+and+secure+telemedicine+powered+by+https://pontis.me"
                           target="_blank">
                           <div class="card emailcard">
                             <div class="card-body">
                               <img src="/dist/img/gmail.jpg" style="width:100%">
                               <p class="emailtext">Use Gmail</p>
                             </div>
                           </div>
                         </a>
                       </div>
                       <div class="link_item">
                         <a href="http://compose.mail.yahoo.com/?&subj=Telemedicine%20meeting%20invitation&body=Hello,%20this%20is%20<%=initial%>.%20<%=name%>%20-%20please%20join%20me%20for%20a%20secure%20video%20call:%20%0D%0Dhttps://pontis.me:5060/index/<%= room_name %>%20%0D%0DUse%20a%20computer%20or%20device%20with%20a%20good%20internet%20connection%20and%20webcam.%20If%20you%20run%20into%20issues%20connecting,%20restart%20your%20computer%20or%20check%20out%20the%20pontis.me%20http://help.pontis.me%20%0D%0DSimple,%20free,%20and%20secure%20telemedicine%20powered%20by%20https://pontis.me"
                           target="_blank">
                           <div class="card emailcard">
                             <div class="card-body">
                               <img src="/dist/img/yahoo.jpeg" style="width:75%">
                               <p class="emailtext">Use Yahoo</p>
                             </div>
                           </div>
                         </a>
                       </div>
                       <div class="link_item">
                         <a href="https://outlook.live.com/owa/?path=/mail/action/compose&?&subject=Telemedicine%20meeting%20invitation&body=Hello,%20this%20is%20<%=initial%>.%20<%=name%>%20-%20please%20join%20me%20for%20a%20secure%20video%20call:%20%0D%0Dhttps://pontis.me:5060/index/<%= room_name %>%20%0D%0DUse%20a%20computer%20or%20device%20with%20a%20good%20internet%20connection%20and%20webcam.%20If%20you%20run%20into%20issues%20connecting,%20restart%20your%20computer%20or%20check%20out%20the%20pontis.me%20http://help.pontis.me%20%0D%0DSimple,%20free,%20and%20secure%20telemedicine%20powered%20by%20https://pontis.me"
                           target="_blank">
                           <div class="card emailcard">
                             <div class="card-body">
                               <img src="/dist/img/outlook.webp" style="width:53%">
                               <p class="emailtext">Use Outlook</p>
                             </div>
                           </div>
                         </a>
                       </div>
                     </div>
                   </div>
                   <div id="doxy_me" class="none">
                     <div class="p-2">
                       <label class="patientlabel">Patient Email</label>
                       <div class="form-group">
                         <input type="email" class="form-control inputbox">
                       </div>

                       <label class="patientlabel">Location</label>
                       <select id="cars" name="category" required>
                         <option value="1" selected="selected">https://pontis.me:5060/index/#<%= room_name %>
                         </option>
                       </select>
                       <label class="patientlabel">The message:</label>
                       <p>Hello, <%= initial %>. <%= name %> has invited you to join a secure video call:</p>
                       <p>https://pontis.me:5060/index/#<%= room_name %></p>
                       <p>Make sure you are using a device with good internet connection and access to camera/audio. If
                         you need any assistance please check out the BellaPontiS Help Center or contact your provider
                         directly.</p>
                     </div>
                     <div class="float-right">
                       <button type="button" class="btn btnstyle " data-dismiss="modal">Cancel</button>
                       <button type="button" class="btn  btnstyle1 ml-2">Upgrade to professional or Clinic</button>
                     </div>
                   </div>

                 </div>
               </div>
             </div>
           </div>

           <!-- calendar Modal -->
           <div class="modal fade" id="exampleModalCenter1" tabindex="-1" role="dialog"
             aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
             <div class="modal-dialog modal-dialog-centered" role="document">
               <div class="modal-content">
                 <div class="modal-header">
                   <h5 class="modal-title" id="exampleModalCenterTitle"><i class="fas fa-calendar-alt mr-3 "></i><span
                       class="inviteheading">Invite via Calendar</span></h5>
                   <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                     <span aria-hidden="true">&times;</span>
                   </button>
                 </div>
                 <div class="modal-body">
                   <form>
                     <label class="patientlabel"> Set date and time</label>
                     <div class="form-group">
                      <input type="date" id="currentDate">
