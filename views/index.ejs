                   <label class="patientlabel">Patient phone number</label>
                   <div class="input-group mb-2">
                     <!-- telephone nuber selctor bar -->
                     <input type="tel" class="form-control inputbox" id="phone" name="phone">
                   </div>

                   <label class="patientlabel" for="cars">Location</label>
                   <select id="location1" name="category" class="inputbox" required>
                      <option value="a" selected="selected">
                        https://pontis.me:5060/index/#<%= room_name %>
                      </option>
                      <option value="b">
                        https://pontis.me:5060/index/#<%= room_name %>
                      </option>
                   </select>
                 </div>
                 <div class="modal-footer">
                   <button type="button" class="btn btnstyle" data-dismiss="modal">Cancel</button>
