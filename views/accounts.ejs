                            <label class="labelname">Preferred language</label>
                          </div>
                          <div class="col-md-6">
                            <select id="preferred_language" name="category" class="inputbox" required>
                              <option value="English" selected="selected">English</option>
                              <option value="French">French</option>
                              <option value="Spanish">Spanish</option>
                              <option value="Arabic">Arabic</option>
                              <option value="Mandrain">Mandrain</option>
                              <option value="Russian">Russian</option>
                            </select>
                            <input type="text" name="UId" id="Uid" value="<%= doc_id %>" style="display: none;">
                          </div>
                          <div class="col-md-2"></div>
                        </div>
                        <div class="text-center">
                          <button type="button" id="personal_up" class="btn btn-info">Update</button>
