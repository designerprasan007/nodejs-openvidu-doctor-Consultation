                    <!-- Modal for change email addreess -->
                    <div class="modal fade" id="change_e-address" tabindex="-1" role="dialog"
                      aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
                      <div class="modal-dialog modal-dialog-centered" role="document">
                        <div class="modal-content w-100">
                          <form id="email_reset">
                          <div class="modal-header" style="background-color:#66CCCC">
                            <h5 class="modal-title text-white" id="exampleModalLongTitle">Email Address</h5>
                            <!-- <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                              <span aria-hidden="true">&times;</span>
                            </button> -->
                          </div>
                          <div class="modal-body">
                            <div class="row">
                              <div class="col-md-5">
                                <label class="labelname">Current Password</label>
                              </div>
                              <div class="col-md-7">
                                <div class="form-group">
                                  <input type="password" class="form-control inputbox" id="cur_pass1" required>
                                </div>
                              </div>
                            </div>
                            <div class="row">
                              <div class="col-md-5">
                                <label class="labelname">New Email</label>
                              </div>
                              <div class="col-md-7">
                                <div class="form-group">
                                  <input type="text" class="form-control inputbox" id="new_email"required>
                                </div>
                              </div>
                            </div>
                            <div class="row">
                              <div class="col-md-5">
                                <label class="labelname">Confirm New Email</label>
                              </div>
                              <div class="col-md-7">
                                <div class="form-group">
                                  <input type="text" class="form-control inputbox" id="c_newemail" required>
                                </div>
                              </div>
                            </div>
                          </div>
                          <div class="modal-footer" style="border-top:none!important;margin-top: -30px;">
                            <button type="button" class="btn btn-info update_button1" id="update_button1" data-dismiss="modal">Update</button>
                          </div>
                        </form>
                        </div>
                      </div>
                    </div>

                    <!-- Modal for change password  -->
                    <div class="modal fade" id="change_password" tabindex="-1" role="dialog"
                      aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
                      <div class="modal-dialog modal-dialog-centered" role="document">
                        <div class="modal-content w-100">
                          <div class="modal-header" style="background-color:#66CCCC">
                            <h5 class="modal-title text-white" id="exampleModalLongTitle">Password</h5>
                            <!-- <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                              <span aria-hidden="true">&times;</span>
                            </button> -->
                          </div>
                          <div class="modal-body">
                            <form id="psw_setup">
                              <div class="row">
                                <div class="col-md-5">
                                  <label class="labelname">Current Password</label>
                                </div>
                                <div class="col-md-7">
                                  <div class="form-group">
                                    <input type="text" class="form-control inputbox" id="cur_pass" required>
                                  </div>

                                </div>
                              </div>
                              <div class="row">
                                <div class="col-md-5">
                                  <label class="labelname">New Password</label>
                                </div>
                                <div class="col-md-7">
                                  <div class="form-group">
                                    <input type="text" class="form-control inputbox"id="new_pass" required>
                                  </div>
                                </div>
                              </div>
                              <div class="row">
                                <div class="col-md-5">
                                  <label class="labelname">Confirm New Password</label>
                                </div>
